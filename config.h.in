#ifndef _config_h_
#define _config_h_ 1

/*
 * $Id: config.h.in,v 1.12 2000-11-26 06:39:54 gnb Exp $
 */

/*
 * The i18n package. Used to find .mo files at runtime.
 */
#define PACKAGE "package"

/*
 * Version of maketool.
 */
#define VERSION "version"

/*
 * Define as non-zero to turn on debugging prints.
 */
#ifndef DEBUG 
#define DEBUG 0
#endif

/*
 * Executable name for GNU make. Can be a full pathname
 * or a basename to be looked up in $PATH at runtime.
 * May contain flags, e.g. "/some/dir/make -x -y -z"
 *
 * BTW if anyone tries to use maketool with ClearCase's
 * make in GNU-make emulation mode, I'd be interested to
 * hear how you fared - Greg.
 */
#define GMAKE "gmake"

/*
 * Whether to define filters to detect errors & warnings
 * generated by MWOS cross-compiler.
 */
#define ENABLE_MWOS 0

/*
 * Whether to include the new and immature online-help feature.
 * This is a temporary **HACK** until the help system stabilises.
 */
#define ENABLE_ONLINE_HELP 0

/*
 * Whether to look in the file /etc/printcap for
 * a list of printers (BSD-derived systems). Defined
 * as 1 or 0.
 */
#define HAVE_ETC_PRINTCAP 0
/*
 * Whether to run the command "lpstat -c" for
 * a list of printers (SysV-derived systems).
 */
#define HAVE_LPSTAT 0
/*
 * Full pathname to the lpstat program if it exists.
 */
#define LPSTAT_COMMAND ""

/* Whether a usable wait3(2) is defined */
#define HAVE_WAIT3 0

/* Various standard autoconf defines */
#define HAVE_MALLOC_H 0
#define HAVE_MEMORY_H 0
#define HAVE_STRCHR 0
#define HAVE_SYS_WAIT_H 0
#define HAVE_SYS_TIME_H 0
#define HAVE_SYS_IOCTL_H 0
#define HAVE_SYS_FILIO_H 0
#define HAVE_SIGNAL_H 0
#define HAVE_REGCOMP 0
#define HAVE_PUTENV 0
#define HAVE_UNISTD_H 0
#define STDC_HEADERS 0
#define RETSIGTYPE void
/* autoconf defines specifically for gettext */
#define ENABLE_NLS 0
#define HAVE_CATGETS 0
#define HAVE_GETTEXT 0
#define HAVE_LC_MESSAGES 0
#define HAVE_STPCPY 0
/*
 * autoconf defines for gettext which are not
 * documented but are necessary anyway.
 */
#undef HAVE_ALLOCA
#undef HAVE_ALLOCA_H
#undef HAVE_ARGZ_H
#undef HAVE_DCGETTEXT
#undef HAVE_GETCWD
#undef HAVE_LIMITS_H
#undef HAVE_LOCALE_H
#undef HAVE_LOCALE_NULL
#undef HAVE_MEMPCPY
#undef HAVE_MMAP
#undef HAVE_MUNMAP
#undef HAVE_NL_TYPES_H
#undef HAVE_SETENV
#undef HAVE_SETLOCALE
#undef HAVE_STRCASECMP
#undef HAVE_STRDUP
#undef HAVE_STRING_H
#undef HAVE_SYS_PARAM_H
#undef HAVE___ARGZ_COUNT
#undef HAVE___ARGZ_NEXT
#undef HAVE___ARGZ_STRINGIFY


#endif /* _config_h_ */
