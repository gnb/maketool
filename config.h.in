#ifndef _config_h_
#define _config_h_ 1

/*
 * $Id: config.h.in,v 1.4 1999-10-16 16:36:56 gnb Exp $
 */

/*
 * The i18n package. Used to find .mo files at runtime.
 */
#define PACKAGE "package"

/*
 * Version of maketool.
 */
#define VERSION "version"

/*
 * Define as non-zero to turn on debugging prints.
 */
#define DEBUG 0

/*
 * Executable name for GNU make. Can be a full pathname
 * or a basename to be looked up in $PATH at runtime,
 * but may *not* contain flags.
 *
 * BTW if anyone tries to use maketool with ClearCase's
 * make in GNU-make emulation mode, I'd be interested to
 * hear how you fared - Greg.
 */
#define GMAKE "make"
/*
 * Basename of GMAKE. Used in filters.
 */
#define GMAKEBASE "make"

/* Various standard autoconf defines */
